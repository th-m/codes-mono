{
  "name": "home",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/home/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build_old": {
      "executor": "@nxext/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/home",
        "frameworkConfigFile": "@nxext/solid/plugins/vite"
      },
      "configurations": {
        "production": {}
      }
    },
    "dev_old": {
      "executor": "@nxext/vite:dev",
      "options": {
        "outputPath": "dist/packages/home",
        "frameworkConfigFile": "@nxext/solid/plugins/vite"
      }
    },
    "build": {
      "command": "solid-start build"
     },
     "dev": {
       "command": "npm --prefix packages/home run dev"
     },
     "start": {
       "command": "solid-start start"
     },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/home/**/*.{ts,spec.ts,tsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/home"],
      "options": {
        "jestConfig": "packages/home/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
